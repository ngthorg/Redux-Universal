parser: 'babel-eslint'

extends: airbnb

plugins:
  - react

ecmaFeatures:
  jsx: true
  experimentalObjectRestSpread: true

env:
  es6: true
  node: true
  browser: true
  jasmine: true
  jest: true

globals:
  window: true

rules:
  no-underscore-dangle: 0
  global-require: 0



  # ...
  no-else-return: 0
  no-param-reassign: 0
